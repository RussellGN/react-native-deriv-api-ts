{
   "name": "react-native-deriv-api-ts",
   "version": "1.0.1",
   "description": "A fork of deriv-api-ts modified for React Native compatibility.",
   "main": "dist/DerivAPI.js",
   "repository": {
      "url": "git+https://github.com/RussellGN/react-native-deriv-api-ts.git",
      "type": "git"
   },
   "author": "Russell Gundani <dudurussell@gmail.com>",
   "license": "MIT",
   "keywords": [
      "deriv",
      "deriv-api",
      "deriv-ts",
      "react-native"
   ],
   "scripts": {
      "type-check": "tsc -p tsconfig.lint.json --noEmit",
      "eslinter": "eslint -c .eslintrc.js ./ --ext .js,.ts",
      "lint": "npm run eslinter; npm run type-check",
      "build": "tsc --build ./tsconfig.json",
      "test": "jest --passWithNoTests",
      "prepack": "npm run build"
   },
   "devDependencies": {
      "@types/node": "^17.0.23",
      "@typescript-eslint/eslint-plugin": "^5.11.0",
      "@typescript-eslint/parser": "^5.11.0",
      "eslint": "8.2.0",
      "jest": "^27.5.1",
      "ts-jest": "^27.1.3",
      "ts-node": "^10.5.0",
      "typescript": "^4.5.5"
   },
   "dependencies": {
      "@deriv/api-types": "^1.0.50",
      "@deriv/deriv-api": "^1.0.9",
      "react-native-debug": "^3.0.0",
      "react-native-websocket": "^1.0.2",
      "rxjs": "^7.5.4"
   },
   "files": [
      "src/",
      "dist/**/*"
   ]
}
